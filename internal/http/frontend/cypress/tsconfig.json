{
  "compilerOptions": {
    "target": "ES2015",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2015", "ES2020.Intl", "DOM", "DOM.Iterable"],
    "types": ["cypress"],
    "isolatedModules": false,
    "baseUrl": "..",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": [
    "../tests/e2e/**/*.ts",
    "./support/**/*.ts",
    "./support/**/*.d.ts"
  ]
}

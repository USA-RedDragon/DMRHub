    "cypress": "^12.6.0",
    "cypress-mochawesome-reporter": "^3.3.0",
    "cypress-multi-reporters": "^1.6.2",
    "cypress-wait-for-stable-dom": "^0.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "mocha-junit-reporter": "^2.2.0",

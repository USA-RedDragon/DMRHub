{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build:sitemap": "node --experimental-strip-types scripts/sitemap.ts",
    "build:tsc": "vue-tsc -b",
    "build": "npm run build:tsc && npm run build:sitemap && vite build",
    "preview": "vite preview --host",
    "screenshot": "start-server-and-test preview :4173 \"cypress run --e2e --headed --browser chrome --config excludeSpecPattern='**/no-op' --spec tests/e2e/screenshots/**.cy.ts\"",
    "test:e2e": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed'",
    "test:e2e:headless": "start-server-and-test preview localhost:4173 'cypress run --e2e --headless'",
    "test:e2e:chrome": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed --browser chrome'",
    "test:e2e:firefox": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed --browser firefox'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' localhost:4173 'cypress open --e2e'",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:dev": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json"
  },
  "dependencies": {
    "@tanstack/vue-table": "8.21.3",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "14.2.1",
    "axios": "1.13.5",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "lucide-vue-next": "0.574.0",
    "pinia": "3.0.4",
    "reka-ui": "2.8.0",
    "tailwind-merge": "3.4.1",
    "tailwindcss": "4.1.18",
    "vue": "3.5.28",
    "vue-router": "5.0.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "4.0.0",
    "@eslint/js": "9.39.2",
    "@tailwindcss/vite": "4.1.18",
    "@tsconfig/node22": "22.0.5",
    "@types/jsdom": "27.0.0",
    "@unhead/vue": "2.1.4",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/eslint-plugin": "1.6.9",
    "@vitest/ui": "4.0.18",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.7.0",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.8.1",
    "cypress": "15.10.0",
    "cypress-mochawesome-reporter": "4.0.2",
    "cypress-multi-reporters": "2.0.5",
    "cypress-wait-for-stable-dom": "0.1.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-cypress": "5.3.0",
    "eslint-plugin-vue": "10.8.0",
    "globals": "17.3.0",
    "jsdom": "28.1.0",
    "mocha-junit-reporter": "2.2.1",
    "mochawesome": "7.1.4",
    "mochawesome-merge": "5.1.1",
    "mochawesome-report-generator": "6.3.2",
    "path-parser": "6.1.0",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "sitemap": "9.0.0",
    "start-server-and-test": "2.1.3",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-istanbul": "7.2.1",
    "vitest": "4.0.18",
    "vue-tsc": "3.2.4"
  },
  "nyc": {
    "report-dir": "coverage/e2e",
    "reporter": [
      "lcov",
      "cobertura",
      "text-summary"
    ]
  }
}
